defaults
auth on
tls on
tls_starttls off
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile ~/.var/log/msmtp.log

account majutsushi.net
host mail.majutsushi.net
port 465
from jan@majutsushi.net
user jan
passwordeval python -c 'import keyring; print keyring.get_password("majutsushi.net", "jan")'

account gmail
host smtp.gmail.com
port 465
from jan.larres@gmail.com
user jan.larres

account default : majutsushi.net
