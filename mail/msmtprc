defaults
auth on
tls on
logfile ~/.cache/msmtp.log

account majutsushi.net
host mail.majutsushi.net
from jan@majutsushi.net
user jan@majutsushi.net
tls_fingerprint 0E:DD:5B:8E:5D:63:30:C9:E4:17:D9:72:D7:6B:C3:87:14:DB:39:A7

account gmail
host smtp.gmail.com
from jan.larres@gmail.com
user jan.larres
tls_trust_file /etc/ssl/certs/ca-certificates.crt

account default : majutsushi.net
