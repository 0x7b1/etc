defaults
auth on
tls on
logfile ~/.cache/msmtp.log

account majutsushi.net
host mail.majutsushi.net
from jan@majutsushi.net
user jan@majutsushi.net
tls_fingerprint AE:22:5D:BD:14:92:05:44:37:DE:8E:A1:FE:F3:8E:62:8F:73:A4:CC

account gmail
host smtp.gmail.com
from jan.larres@gmail.com
user jan.larres
tls_trust_file /etc/ssl/certs/ca-certificates.crt

account default : majutsushi.net
