"source ~/.vimrc

"highlight Normal guibg=black guifg=white
colorscheme desert

" Set font for GTK2 version of gvim
set guifont=Monospace\ 8
set guioptions+=c
set guioptions-=T
set guioptions-=e

"let &guicursor = &guicursor . ",a:blinkon0"

set nu

set lines=50
set columns=95
winpos 310 70

set listchars=tab:»·,trail:␣

